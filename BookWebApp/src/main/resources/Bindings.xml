<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	schemaLocation="BookService.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_1_0.xsd"
	jaxb:version="2.0">
	
	<jaxb:bindings node="/xsd:schema/xsd:element[@name='book']">
		<jaxb:class name="BookState"/>
	</jaxb:bindings>
	
	<jaxb:bindings node="/xsd:schema/xsd:element[@name='books']">
		<jaxb:class name="BooksState"/>
	</jaxb:bindings>
	
	<jaxb:bindings node="/xsd:schema/xsd:element[@name='reviews']">
		<jaxb:class name="ReviewsState"/>
	</jaxb:bindings>	
	
	<jaxb:bindings node="/xsd:schema/xsd:element[@name='review']">
		<jaxb:class name="ReviewState"/>
	</jaxb:bindings>
	
</jaxb:bindings>